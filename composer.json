{
    "name": "mindtwo/laravel-decorator",
    "description": "Decorators for Eloquent Models in Laravel",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://www.mindtwo.de/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "laravel/framework": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "mindtwo\\LaravelDecorator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mindtwo\\LaravelDecorator\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "mindtwo\\LaravelDecorator\\Providers\\DecoratorServiceProvider"
            ]
        }
    }
}
